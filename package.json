{
  "name": "backendProject",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "cross-env NODE_ENV=development nodemon --ext ts --exec ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint --flag unstable_ts_config",
    "start": "cross-env NODE_ENV=production node -r ./dist/config.js ./dist",
    "test": "NODE_ENV=test ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.20.1",
    "@prisma/client": "^6.6.0",
