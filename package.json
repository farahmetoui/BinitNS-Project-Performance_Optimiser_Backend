    "bcryptjs": "^3.0.2",
    "bson": "^6.10.3",
    "chrome-launcher": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crawlee": "^3.13.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-schema": "^1.4.3",
    "jet-validators": "^1.0.17",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lighthouse": "^12.4.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.15.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "posthog-js": "^1.223.3",
    "posthog-node": "^4.7.0",
    "prisma": "^6.6.0",
    "puppeteer": "^24.3.0",
