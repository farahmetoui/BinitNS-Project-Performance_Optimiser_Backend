    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.6",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/mongodb": "^4.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.4",
